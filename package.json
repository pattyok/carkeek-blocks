{
    "name": "mytheme-blocks",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "checkFormat": "prettier --check \"**/*.{js,json,css,scss}\"",
        "format": "prettier --write \"**/*.{js,json,css,scss}\"",
        "lint": "eslint src",
        "start": "webpack --mode=development --watch",
        "build": "npm run lint && webpack --mode=production && gulp bundle"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alialaa/gutenberg-course-webpack.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "linters": {
            "src/**/*.js": "eslint",
            "**/*.{js,json,css,scss}": [
                "prettier --write",
                "git add"
            ]
        }
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/alialaa/gutenberg-course-webpack/issues"
    },
    "homepage": "https://github.com/alialaa/gutenberg-course-webpack#readme",
    "dependencies": {
        "@wordpress/blob": "^2.4.0",
        "@wordpress/blocks": "^6.2.2",
        "@wordpress/compose": "^3.3.0",
        "@wordpress/data": "^4.5.0",
        "@wordpress/edit-post": "^3.4.0",
        "@wordpress/html-entities": "^2.3.0",
        "@wordpress/i18n": "^3.3.0",
        "@wordpress/plugins": "^2.3.0",
        "jquery": "^3.4.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-sortable-hoc": "^1.9.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@wordpress/browserslist-config": "^2.3.0",
        "@wordpress/components": "^7.3.1",
        "@wordpress/editor": "^9.2.4",
        "@wordpress/element": "^2.3.0",
        "autoprefixer": "^9.5.1",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "browserslist": "^4.5.4",
        "classnames": "^2.2.6",
        "clean-webpack-plugin": "^2.0.1",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-react": "^7.12.4",
        "gulp": "^4.0.0",
        "gulp-zip": "^4.2.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "loadash": "^1.0.0",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "^4.13.1",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.17.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.2"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ]
}
