{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "carkeek-blocks/featured-image",
  "title": "Featured Image",
  "category": "carkeek-category",
  "description": "Place the featured image within your page",
  "keywords": [
    "image",
    "featured"
  ],
  "version": "1.0.0",
  "textdomain": "carkeek-blocks",
  "attributes": {
    "focalPoint": {
      "type": "object"
    },
    "blockId": {
      "type": "string"
    },
    "imageSize": {
      "type": "string"
    },
    "className": {
      "type": "string"
    },
    "showCaption": {
      "type": "boolean",
      "default": false
    }
  },
  "styles": [
    {
      "name": "landscape",
      "label": "Landscape",
      "isDefault": true
    },
    {
      "name": "square",
      "label": "Square"
    },
    {
      "name": "portrait",
      "label": "Portrait"
    },
    {
      "name": "no-crop",
      "label": "No Crop"
    }
  ],
  "supports": {
    "align": [
      "wide",
      "full",
      "left",
      "center",
      "right"
    ],
    "anchor": false,
    "spacing": true
  },
  "editorScript": "file:./index.js"
}