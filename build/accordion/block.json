{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "carkeek-blocks/accordion",
  "version": "2.0.1",
  "title": "Accordion Block",
  "category": "widgets",
  "description": "Accordion block.",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "keywords": [
    "accordion",
    "toggle",
    "expand",
    "colllapse"
  ],
  "styles": [
    {
      "name": "default",
      "label": "Standard Accordion",
      "isDefault": true
    },
    {
      "name": "mini",
      "label": "Mini Style"
    }
  ],
  "providesContext": {
    "carkeek-blocks/isFAQ": "isFAQ",
    "carkeek-blocks/headerStyle": "headerStyle"
  },
  "textdomain": "carkeek-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "headerStyle": {
      "type": "string",
      "default": "h3"
    },
    "openMultiple": {
      "type": "boolean",
      "default": true
    },
    "isFAQ": {
      "type": "boolean",
      "default": false
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {},
          "padding": {}
        }
      }
    }
  }
}